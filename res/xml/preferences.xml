            <intent
                android:targetClass="com.roboo.like.netease.BindActivity"
                android:targetPackage="com.roboo.like.netease" />
        </PreferenceScreen>

        <SwitchPreference
            android:key="tv_news_push"
            android:title="@string/tv_news_push" />
        <SwitchPreference
            android:key="@string/tv_wifi_download"
            android:title="@string/tv_wifi_download" />
        <SwitchPreference
            android:key="@string/tv_2g_3g_no_img"
            android:title="@string/tv_2g_3g_no_img" />
        <SwitchPreference
            android:key="@string/tv_auto_load_more"
            android:title="@string/tv_auto_load_more" />

        <Preference
            android:key="@string/tv_manual_clear_cache"
            android:title="@string/tv_manual_clear_cache" />
    </PreferenceCategory>
