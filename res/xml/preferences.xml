<?xml version="1.0" encoding="utf-8"?>
<!-- This is a primitive example showing the different types of preferences available. -->

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- account  settings -->
    <PreferenceCategory android:title="@string/tv_account_setting" >
        <PreferenceScreen
            android:key="tv_account"
            android:title="@string/tv_account" >
            <intent
                android:targetClass="com.roboo.like.netease.AccountActivity"
                android:targetPackage="com.roboo.like.netease" />
        </PreferenceScreen>
        <PreferenceScreen
            android:key="tv_bind"
            android:title="@string/tv_bind" >
            <intent
                android:targetClass="com.roboo.like.netease.BindActivity"
                android:targetPackage="com.roboo.like.netease" />
        </PreferenceScreen>
    </PreferenceCategory>
